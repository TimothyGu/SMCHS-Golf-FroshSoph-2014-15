<table class="table sortable-theme-bootstrap" data-sortable>
<% var short = {
     'Tijeras Creek'      : 'TC'
   , 'Casta del Sol'      : 'CdS'
   , 'Los Serranos'       : 'LS'
   , 'San Juan Hills'     : 'SJH'
   , 'Meadowlark'         : 'M'
   , 'Willowick'          : 'W'
   , 'Tustin Ranch'       : 'TR'
   , 'Links @ Summerly'   : 'L@S'
   , 'Black Gold'         : 'BG'
   , 'Arroyo Trabuco'     : 'AT'
   , 'Los Amigos'         : 'LA'
   , 'Little Rec'         : 'LR'
   , 'Total # of matches' : 'Tot'
   , 'Average'            : 'Avg'
   , 'Match average'      : 'MAvg'
   , 'Improvement'        : 'Imp'
   } -%>
<% function rank (arr) {
     return arr.slice().map(function(v){ return sorted.indexOf(v)+1 })
   } -%>
<thead>
  <tr>
    <th>Player</th>
    <% for (var i = 0; i < courses.length; i ++) { -%>
      <th><%= short[courses[i]] %></th>
    <% } -%>
    <th>Tot</th>
    <th>Avg</th>
    <th>MAvg</th>
    <th>Imp</th>
  </tr>
</thead>
<tbody>
  <% for (var i = 0; i < players.length; i ++) { -%>
    <% var data = players[i] -%>
    <% var tot = 0, sum, mSum = 0 -%>
    <tr>
      <th data-sortable="false"
          data-value="<%= data.label.split(' ').reverse().join(', ') %>">
        <%= data.label.split(' ')[1] %>
      </th>
      <td><%= data.data[0] %></td>
      <% sum = data.data[0] -%>
      <% for (var j = 1; j < courses.length; j ++) { -%>
        <% if (data.data[j] == null) { -%>
          <td data-value="9999999999"></td>
          <% continue -%>
        <% } -%>
        <td>
          <%= data.data[j] %>
          <% tot ++ -%>
          <% sum += data.data[j] -%>
          <% mSum += data.data[j] -%>
        </td>
      <% } -%>
      <th data-sortable="false"><%= tot %></th>
      <th data-sortable="false"><%= ( sum / (tot + 1)               ).toFixed(2) %></th>
      <th data-sortable="false"><%= (mSum /  tot                    ).toFixed(2) %></th>
      <th data-sortable="false"><%= (mSum /  tot      - data.data[0]).toFixed(2) %></th>
    </tr>
  <% } -%>
</tbody>
</table>
<dl class="dl-horizontal">
  <dt>Shorthands</dt><dd></dd>
  <% var keys = Object.keys(short) -%>
  <% for (var i = 0; i < keys.length; i ++) { -%>
    <dt><%= short[keys[i]] %></dt>
    <dd><%= keys[i] %></dd>
  <% } -%>
</dl>
